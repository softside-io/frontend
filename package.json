{
  "name": "softside-app",
  "version": "0.0.1",
  "author": "Softside",
  "homepage": "https://ionicframework.com/",
  "scripts": {
    "gc": "node ./projects/softside/ui-sdk/.templates/generate-component.mjs",
    "commit": "node .scripts/commit.mjs",
    "ng": "ng",
    "ionic": "ionic",
    "start:lib": "concurrently \"tailwind --watch -c ./projects/softside/ui-sdk/tailwind.config.js -o ./projects/softside/ui-sdk/scss/tailwind.scss -p\" \"ng build @softside/ui-sdk --watch --configuration development\"",
    "start:web": "ionic serve --project web",
    "start:mobile": "ionic serve --project mobile",
    "build:lib": "tailwind -c ./projects/softside/ui-sdk/tailwind.config.js -o ./projects/softside/ui-sdk/scss/tailwind.scss && ng build @softside/ui-sdk",
    "build:web": "env-cmd -e production_web ng build web",
    "build:mobile": "env-cmd -e production_mobile ng build mobile",
    "deploy:local": "servor public/browser/ index.html 4321 --browse --secure",
    "test": "ng test",
    "lint": "ng lint",
    "prepare": "husky install",
    "stylelint": "stylelint",
    "lint:styles": "stylelint \"projects/**/*.scss\"",
    "lint:styles-fix": "stylelint \"projects/**/*.scss\" --fix",
    "prettier:ts": "npx prettier --write projects/**/*.ts",
    "prettier:html": "npx prettier --write projects/**/*.html",
    "prettier:scss": "npx prettier --write projects/**/*.scss",
    "prettier:all": "npx prettier --write projects/**/*.{ts,html,scss}",
    "docs:lib:storybook": "tailwind -c ./projects/softside/ui-sdk/tailwind.config.js -o ./projects/softside/ui-sdk/scss/tailwind.scss && ng run @softside/ui-sdk:storybook",
    "docs:lib:storybook-documentation": "npx compodoc -p projects/softside/ui-sdk/.storybook/tsconfig.json -e json -d projects/softside/ui-sdk",
    "docs:lib:build-storybook": "ng run lib:build-storybook",
    "docs:lib:compodoc": "npx compodoc -p projects/softside/ui-sdk/tsconfig.doc.json -d docs/softside/ui-sdk -s --port 8085",
    "docs:mobile:storybook": "ng run mobile:storybook",
    "docs:mobile:storybook-documentation": "npx compodoc -p projects/mobile/.storybook/tsconfig.json -e json -d projects/mobile",
    "docs:mobile:build-storybook": "ng run mobile:build-storybook",
    "docs:mobile:compodoc": "npx compodoc -p projects/mobile/tsconfig.doc.json -d docs/mobile -s --port 8086",
    "docs:web:storybook": "ng run web:storybook",
    "docs:web:storybook-documentation": "npx compodoc -p projects/web/.storybook/tsconfig.json -e json -d projects/web",
    "docs:web:build-storybook": "ng run web:build-storybook",
    "docs:web:compodoc": "npx compodoc -p projects/web/tsconfig.doc.json -d docs/web -s --port 8087",
    "api:gen": "shx rm -rf projects/api/* && openapi -i http://localhost:3000/docs-yaml -o projects/api -c angular && node .scripts/post-swagger.mjs",
    "pkg:check": "npx npm-check-updates",
    "pkg:upgrade": "npx npm-check-updates -u"
  },
  "private": true,
  "dependencies": {
    "@abacritt/angularx-social-login": "^2.2.0",
    "@angular/animations": "^17.2.3",
    "@angular/common": "^17.2.3",
    "@angular/compiler": "^17.2.3",
    "@angular/core": "^17.2.3",
    "@angular/forms": "^17.2.3",
    "@angular/platform-browser": "^17.2.3",
    "@angular/platform-browser-dynamic": "^17.2.3",
    "@angular/router": "^17.2.3",
    "@capacitor/app": "5.0.7",
    "@capacitor/core": "5.7.1",
    "@capacitor/haptics": "5.0.7",
    "@capacitor/keyboard": "5.0.8",
    "@capacitor/status-bar": "5.0.7",
    "@ionic/angular": "^7.7.3",
    "@ionic/storage-angular": "^4.0.0",
    "@ngx-formly/core": "^6.3.0",
    "@ngx-formly/ionic": "^6.3.0",
    "ion-intl-tel-input": "^1.0.5",
    "ionicons": "^7.2.2",
    "ng-let": "^17.0.4",
    "ngx-image-cropper": "^7.2.1",
    "ngx-softside-table": "^1.0.0",
    "rxjs": "~7.8.1",
    "tslib": "^2.6.2",
    "zone.js": "~0.14.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.2.2",
    "@angular-eslint/builder": "17.2.1",
    "@angular-eslint/eslint-plugin": "17.2.1",
    "@angular-eslint/eslint-plugin-template": "17.2.1",
    "@angular-eslint/schematics": "17.2.1",
    "@angular-eslint/template-parser": "17.2.1",
    "@angular/cdk": "^17.2.1",
    "@angular/cli": "^17.2.2",
    "@angular/compiler-cli": "^17.2.3",
    "@angular/language-service": "^17.2.3",
    "@babel/plugin-proposal-private-methods": "^7.18.6",
    "@capacitor/cli": "5.7.1",
    "@commitlint/cli": "^19.0.3",
    "@commitlint/config-conventional": "^19.0.3",
    "@compodoc/compodoc": "^1.1.23",
    "@ionic/angular-toolkit": "^11.0.1",
    "@ionic/cli": "^7.2.0",
    "@ngx-formly/schematics": "^6.3.0",
    "@storybook/addon-docs": "^7.6.17",
    "@storybook/addon-essentials": "^7.6.17",
    "@storybook/addon-interactions": "^7.6.17",
    "@storybook/addon-links": "^7.6.17",
    "@storybook/addon-styling-webpack": "^0.0.6",
    "@storybook/angular": "^7.6.17",
    "@storybook/blocks": "^7.6.17",
    "@storybook/testing-library": "^0.2.2",
    "@types/jasmine": "~5.1.4",
    "@types/node": "^20.11.24",
    "@typescript-eslint/eslint-plugin": "7.1.0",
    "@typescript-eslint/parser": "7.1.0",
    "autoprefixer": "^10.4.18",
    "chalk": "^5.3.0",
    "check-dependencies": "^2.0.0",
    "concurrently": "^8.2.2",
    "env-cmd": "^10.1.0",
    "eslint": "^8.57.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-deprecation": "^2.0.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-storybook": "^0.8.0",
    "eslint-plugin-unused-imports": "^3.1.0",
    "husky": "^9.0.11",
    "jasmine-core": "~5.1.2",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.4.3",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "lint-staged": "^15.2.2",
    "ng-packagr": "^17.2.0",
    "openapi-typescript-codegen": "^0.27.0",
    "postcss": "^8.4.35",
    "prettier": "^3.2.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "servor": "^4.0.2",
    "shx": "^0.3.4",
    "storybook": "^7.6.17",
    "stylelint": "^16.2.1",
    "stylelint-config-standard": "^36.0.0",
    "stylelint-scss": "^6.1.0",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "description": "An Ionic project",
  "lint-staged": {
    "*.ts": [
      "eslint"
    ],
    "*.{.css,scss}": "stylelint --fix"
  }
}
